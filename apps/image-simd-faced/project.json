{
    "name": "image-simd-faced",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "apps/image-simd-faced/src",
    "projectType": "application",
    "targets": {
        "build": {
            "executor": "@nx/vite:build",
            "outputs": [
                "{options.outputPath}"
            ],
            "defaultConfiguration": "production",
            "options": {
                "outputPath": "dist/apps/image-simd-faced"
            },
            "configurations": {
                "development": {
                    "mode": "development"
                },
                "production": {
                    "mode": "production"
                }
            }
        },
        "serve": {
            "executor": "@nx/vite:dev-server",
            "defaultConfiguration": "development",
            "options": {
                "buildTarget": "image-simd-faced:build"
            },
            "configurations": {
                "development": {
                    "buildTarget": "image-simd-faced:build:development",
                    "hmr": true
                },
                "production": {
                    "buildTarget": "image-simd-faced:build:production",
                    "hmr": false
                }
            }
        },
        "preview": {
            "executor": "@nx/vite:preview-server",
            "defaultConfiguration": "development",
            "options": {
                "buildTarget": "image-simd-faced:build"
            },
            "configurations": {
                "development": {
                    "buildTarget": "image-simd-faced:build:development"
                },
                "production": {
                    "buildTarget": "image-simd-faced:build:production"
                }
            }
        },
        "test": {
            "executor": "@nx/vite:test",
            "outputs": [
                "coverage/apps/image-simd-faced"
            ],
            "options": {
                "passWithNoTests": true,
                "reportsDirectory": "../../coverage/apps/image-simd-faced"
            }
        },
        "lint": {
            "executor": "@nx/linter:eslint",
            "outputs": [
                "{options.outputFile}"
            ],
            "options": {
                "lintFilePatterns": [
                    "apps/image-simd-faced/**/*.{ts,tsx,js,jsx}"
                ]
            }
        },
        "serve-static": {
            "executor": "@nx/web:file-server",
            "options": {
                "buildTarget": "image-simd-faced:build"
            }
        },
        "docker-build": {
            "dependsOn": [
                "build"
            ],
            "executor": "nx:run-commands",
            "options": {
                "commands": [
                    "docker build -f apps/image-simd-faced/Dockerfile . -t image-simd-faced:latest"
                ]
            }
        }
    },
    "tags": []
}